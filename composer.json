{
  "name": "catoth/antragsgruen",
  "description": "AntragsgrÃ¼n",
  "type": "project",
  "license": "AGPL-3.0-only",
  "support": {
    "issues": "https://github.com/CatoTH/antragsgruen/issues",
    "source": "https://github.com/CatoTH/antragsgruen"
  },
  "keywords": [
    "motions",
    "amendments",
    "party"
  ],
  "homepage": "https://antragsgruen.de/",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.2.0",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "catoth/html2opendocument": "0.19",
    "colinodell/json5": "^3.0",
    "endroid/qr-code": "^5.0",
    "ezyang/htmlpurifier": "dev-main as v4.19.1",
    "firebase/php-jwt": "^7.0",
    "guzzlehttp/guzzle": "^7.7",
    "league/oauth2-client": "^2.9",
    "paragonie/sodium_compat": "^2.0",
    "phpoffice/phpspreadsheet": "^5.0",
    "predis/predis": "^3.0",
    "s1syphos/php-simple-captcha": "dev-main#26d63ec7d3da30ea214d796eb613a8b6c2c51d81 as 2.4.0",
    "scssphp/scssphp": "^2.0",
    "setasign/fpdi": "^2.3.7",
    "spomky-labs/otphp": "^11.4",
    "symfony/amazon-mailer": "^6.4",
    "symfony/lock": "^6.4",
    "symfony/mailer": "^6.4",
    "symfony/mailjet-mailer": "^6.4",
    "symfony/polyfill-iconv": "^1.27",
    "symfony/property-access": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/string": "^6.4",
    "symfony/translation-contracts": "^3.0",
    "tecnickcom/tcpdf": "^6.6.2",
    "tijsverkoyen/css-to-inline-styles": "^2.3",
    "yiisoft/yii2": "~2.0.47",
    "yiisoft/yii2-authclient": "^2.2.14",
    "yiisoft/yii2-redis": "^2.0"
  },
  "require-dev": {
    "codeception/codeception": "^5.2",
    "codeception/module-asserts": "^3.0.0",
    "codeception/module-phpbrowser": "^3.0.0",
    "codeception/module-webdriver": "^4.0.2",
    "codeception/module-yii2": "^1.1.8",
    "phpmd/phpmd": "^2.13.0",
    "phpstan/phpstan": "^2.1.33",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "squizlabs/php_codesniffer": "^3.7.2",
    "yiisoft/yii2-debug": "^2.1.23"
  },
  "conflict": {
    "psr/cache": ">=3.0.0",
    "symfony/config": ">=7.0.0",
    "symfony/console": ">=7.0.0",
    "symfony/dependency-injection": ">=7.0.0",
    "symfony/filesystem": ">=7.0.0",
    "symfony/finder": ">=7.0.0",
    "symfony/var-exporter": ">=7.0.0",
    "symfony/yaml": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "setasign\\FpdiPdfParser\\": "components/fpdi/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "suggest": {
    "yiisoft/yii2-redis": "Allows using redis for caching",
    "simplesamlphp/simplesamlphp": "Allowed Single Sign On Authentication using SAML",
    "predis/predis": "If simplesaml should store its data to redis"
  },
  "config": {
    "platform": {
      "php": "8.2.30"
    },
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "simplesamlphp/composer-module-installer": true,
      "simplesamlphp/composer-xmlprovider-installer": true,
      "yiisoft/yii2-composer": true
    }
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0775",
          "web/assets": "0755",
          "yii": "0755"
        }
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/CatoTH/php-simple-captcha.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/CatoTH/htmlpurifier.git"
    }
  ]
}
